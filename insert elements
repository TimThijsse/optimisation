//Voorbeeld van element verplaatsen:

var plek = document.querySelector('.categories-heading');

//insert selected element
plek.insertAdjacentElement('afterend', var);

//insert html
plek.insertAdjacentHTML('beforeend', "bericht");

//change inner HTML
plek.innerHTML = "ok";

